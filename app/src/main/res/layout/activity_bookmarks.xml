<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.owner.petbetter.activities.BookmarksActivity">

    <include
        android:id="@+id/bookmarkToolbar"
        layout="@layout/toolbar_viewpost"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        />
    <include android:id="@+id/toolbarBook"
        layout="@layout/bookmark_toolbar"
        android:layout_height="?attr/actionBarSize"
        android:layout_width="match_parent"
        android:layout_below="@+id/bookmarkToolbar">
    </include>

    <FrameLayout
        android:id="@+id/bookmark_container"
        android:layout_width="100dp"
        android:layout_height="100dp"
        tools:layout_editor_absoluteY="18dp"
        tools:layout_editor_absoluteX="8dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_below="@+id/toolbarBook">


    </FrameLayout>
</RelativeLayout>
